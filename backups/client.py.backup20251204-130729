# client.py
import socket

HOST = "127.0.0.1"   # server address
PORT = 65432         # same port as server

dummy_payload = "Hello from client! This is a dummy payload."

with socket.socket(socket.AF_INET, socket.SOCK_STREAM) as s:
    s.connect((HOST, PORT))
    s.sendall(dummy_payload.encode())
    data = s.recv(1024)
    print(f"Server replied: {data.decode()}")
